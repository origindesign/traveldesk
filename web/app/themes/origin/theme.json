  "styles": {
    "blocks": {},
    "color": {
      "text": "var(--wp--preset--color--base)"
    },
    "elements": {
      "link": {
        "color": {
          "text": "var(--wp--preset--color--contrast)"
        },
        ":hover": {
          "color": {
            "text": "var(--wp--preset--color--contrast)"
          },
          "typography": {
            "textDecoration": "none"
          }
        },
        ":focus": {
          "color": {
            "text": "var(--wp--preset--color--contrast)"
          }
        },
        ":active": {
          "color": {
            "text": "var(--wp--preset--color--contrast)"
          }
        }
      }
    },
    "typography": {
      "fontSize": "var(--wp--preset--font-size--medium)",
      "fontFamily": "var(--wp--preset--font-family--system)",
      "lineHeight": "1.7"
    },
    "spacing": {
      "blockGap": false,
      "padding": false
    }
  },
  "templateParts": [
    {
      "name": "header",
      "title": "Header",
      "area": "header"
    },
    {
      "name": "footer",
      "title": "Footer",
      "area": "footer"
    }
  ],
  "customTemplates": [
    {
      "name": "no-title",
      "title": "No title",
      "postTypes": ["post", "page"]
    },
    {
      "name": "featured-image",
      "title": "Featured image in header",
