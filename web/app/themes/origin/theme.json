          "light": 100,
          "normal": 200,
          "bold": 800
        }
      },
      "grid": {
        "gutter-large": "6rem",
        "gutter-medium": "4rem",
        "gutter": "2rem",
        "gutter-sm": "1.7rem",
        "containers": {
          "xs": "100%",
          "sm": "90%",
          "md": "90%",
          "lg": "160rem"
        }
      }
    },
    "useRootPaddingAwareAlignments": true
